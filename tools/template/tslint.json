{
  "extends": "tslint:recommended",
  "rules": {
    "object-literal-sort-keys": false,
    "quotemark": [ true, "single", "jsx-double" ],
    "class-name": false,
    "variable-name": false,
    "member-access": false,
    "member-ordering": [false],
    "no-namespace": [false],
    "interface-name": false,
    "no-unused-variable": true
  }
}